<config>
  <enabled>true</enabled>
  <name>{hostname}</name>
  <listeners>all:8999</listeners>
  <maxRequests>50</maxRequests>
  <historySize>100</historySize>
  <slowHistorySize>100</slowHistorySize>
  <slowPageThreshold>5000</slowPageThreshold>
  <slowQueryThreshold>1000</slowQueryThreshold>
  <dashboardMinSlowRequests>5</dashboardMinSlowRequests>
  <clickableURLs>true</clickableURLs>
  <cloudwatchEnabled>false</cloudwatchEnabled>
</config>
